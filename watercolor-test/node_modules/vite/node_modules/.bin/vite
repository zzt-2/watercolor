#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/code/project/watercolor/watercolor-test/node_modules/.pnpm/vite@4.5.13_@types+node@18.19.86/node_modules/vite/bin/node_modules:/mnt/d/code/project/watercolor/watercolor-test/node_modules/.pnpm/vite@4.5.13_@types+node@18.19.86/node_modules/vite/node_modules:/mnt/d/code/project/watercolor/watercolor-test/node_modules/.pnpm/vite@4.5.13_@types+node@18.19.86/node_modules:/mnt/d/code/project/watercolor/watercolor-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/code/project/watercolor/watercolor-test/node_modules/.pnpm/vite@4.5.13_@types+node@18.19.86/node_modules/vite/bin/node_modules:/mnt/d/code/project/watercolor/watercolor-test/node_modules/.pnpm/vite@4.5.13_@types+node@18.19.86/node_modules/vite/node_modules:/mnt/d/code/project/watercolor/watercolor-test/node_modules/.pnpm/vite@4.5.13_@types+node@18.19.86/node_modules:/mnt/d/code/project/watercolor/watercolor-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
